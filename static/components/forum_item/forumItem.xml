<?xml version="1.0" encoding="UTF-8" ?>
<templates xml:space="preserve">
    <t t-name="ForumItem">
        <div class="o_forum" t-att-data-id="props.forumItem.id">
            <img t-att-src="props.forumItem.image" />
            <div>
                <h4><t t-out="props.forumItem.name"/></h4>
                <div>
                    <t t-out="props.forumItem.description"/>
                </div>
            </div>
        </div>
    </t>
</templates>